{
	"compilerOptions": {
		"lib": ["ESNext"],
		"module": "esnext",
		"target": "esnext",
		"moduleResolution": "Bundler",
		"moduleDetection": "force",
		"allowImportingTsExtensions": true,
		"strict": true,
		"downlevelIteration": true,
		"skipLibCheck": true,
		"allowSyntheticDefaultImports": true,
		"forceConsistentCasingInFileNames": true,
		"allowJs": false,
		"noEmit": true
	}
}
